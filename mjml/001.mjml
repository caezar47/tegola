<mjml>
    <mj-head>
        <mj-include path="header/style_base.mjml" />
        <mj-attributes>
            <mj-all
                    font-family="Arial, sans-serif"
                    color="#fff"
            />
            <mj-class name="grid"
                      padding="40px 8px 30px"
            />
            <mj-class name="footer__divider"
                      padding="30px 0 0"
            />
            <mj-class name="footer"
                      full-width="full-width"
                      background-color="#c31514"
                      padding="0"
            />
            <mj-class name="footer__inner"
                      padding="20px 8px"
            />
            <mj-class name="footer__grid"
                      padding="0"
            />
            <mj-class name="footer__grid-col2"
                      padding="10px 8px"
                      width="292px"
            />
            <mj-class name="footer__order"
                      color="#c31514"
                      background-color="#fff"
                      border-radius="0px"
                      text-transform="uppercase"
                      font-weight="700"
                      font-size="24px"
                      line-height="32px"
                      width="100%"
                      padding="0 0 10px"
                      inner-padding="15px 20px"
            />
            <mj-class name="footer__store"
                      color="#fff"
                      background-color="transparent"
                      border-radius="0px"
                      font-weight="700"
                      font-size="28px"
                      line-height="38px"
                      width="100%"
                      padding="0"
                      inner-padding="0"
            />
            <mj-class name="footer__text"
                      font-weight="700"
                      font-size="14px"
                      line-height="21px"
                      padding="0 0 10px"
                      align="center"
            />
            <mj-class name="footer__phone"
                      color="#fff"
                      background-color="transparent"
                      border-radius="0px"
                      font-weight="700"
                      font-size="28px"
                      line-height="38px"
                      width="100%"
                      padding="0"
                      inner-padding="0"
            />
            <mj-class name="footer__unsubscribe"
                      color="#fff"
                      font-size="12px"
                      line-height="18px"
                      padding="15px 0 0 0"
                      align="center"
